#!/bin/sh
# ZEO instance start script

PYTHON="/home/dennis/REPOS/zope/bin/python3.7"
INSTANCE_HOME="/home/dennis/REPOS/zope/zeo"
ZODB3_HOME="/home/dennis/REPOS/zope/lib/python3.7/site-packages"

CONFIG_FILE="/home/dennis/REPOS/zope/zeo/etc/zeo.conf"

PYTHONPATH="$ZODB3_HOME"
export PYTHONPATH INSTANCE_HOME

exec "$PYTHON" -m ZEO.runzeo -C "$CONFIG_FILE" ${1+"$@"}
