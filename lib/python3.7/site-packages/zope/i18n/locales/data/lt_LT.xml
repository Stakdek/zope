<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.1/ldml.dtd">
<ldml>
	<identity>
		<version number="1.1"/>
		<generation date="2004-06-04" />
		<language type="lt" /> 
		<territory type="LT" /> 
	</identity>
	<numbers>
		<decimalFormats>
			<decimalFormatLength >
				<decimalFormat >
					<pattern>##,##0.##;-##,##0.##</pattern>
				</decimalFormat>
			</decimalFormatLength >
		</decimalFormats>
		<scientificFormats>
			<scientificFormatLength >
				<scientificFormat >
					<pattern>#E0</pattern>
				</scientificFormat>
			</scientificFormatLength >
		</scientificFormats>
		<percentFormats>
			<percentFormatLength >
				<percentFormat >
					<pattern>#,##%</pattern>
				</percentFormat>
			</percentFormatLength >
		</percentFormats>
		<currencyFormats>
			<currencyFormatLength >
				<currencyFormat >
					<pattern>#,##0.00 ¤;-#,##0.00 ¤</pattern>
				</currencyFormat>
			</currencyFormatLength >
		</currencyFormats>
	</numbers>
</ldml>
